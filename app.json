{
  "expo": {
    "name": "wesapp",
    "slug": "wesapp-new",
    "version": "1.0.0",
    "scheme": "wesapp",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/images/icon.png",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#0f0f0f",
      "image": "./assets/images/splash-icon.png"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wesapp.wesapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000"
      },
      "package": "com.wesapp.wesapp"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "resizeMode": "contain",
          "backgroundColor": "#0f0f0f",
          "image": "./assets/images/splash-icon.png",
          "dark": {
            "resizeMode": "contain",
            "backgroundColor": "#0f0f0f",
            "image": "./assets/images/splash-icon.png"
          },
          "imageWidth": 150
        }
      ],
      [
        "expo-dev-launcher",
        {
          "launchMode": "most-recent"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7f03dacb-a8a6-4292-afbe-86c4c63783c9"
      }
    },
    "owner": "sneezy-01"
  }
}
