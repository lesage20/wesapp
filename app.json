{
  "expo": {
    "name": "wesapp",
    "slug": "wesapp",
    "version": "1.0.0",
    "scheme": "wesapp",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/images/icon.png",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wesapp.wesapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000"
      },
      "package": "com.wesapp.wesapp"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "resizeMode": "contain",
          "backgroundColor": "#4A4A4A",
          "image": "./assets/images/splash-icon.png",
          "dark": {
            "resizeMode": "contain", 
            "backgroundColor": "#4A4A4A",
            "image": "./assets/images/splash-icon.png"
          },
          "imageWidth": 150
        }
      ],
      [
        "expo-dev-launcher",
        {
          "launchMode": "most-recent"
        }
      ],
      [
        "expo-audio",
        {
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png",
          "color": "#4630EB",
          "sounds": [],
          "mode": "production",
          "androidCollapsedTitle": "WeSapp",
          "iosDisplayInForeground": true
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },

    
    "assetBundlePatterns": [
      "**/*"
    ],
    
    "extra": {
      "router": {},
      "eas": {
        "projectId": "063fa890-468c-4505-afae-301ea98b6223"
      }
    }
  }
}
